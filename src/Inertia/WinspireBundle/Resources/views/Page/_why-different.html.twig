<script>
$(function() {
    $('.icon-nav').find('a').append('<span class="background"></span>');
    
    $('.icon-nav a, .text-nav a').on('mouseenter', function(e) {
        if(!$(this).parent().hasClass('active')) {
            var c = $(this).attr('href').substring(1);
            $('li.' + c).addClass('hover');
        }
    });
    
    $('.icon-nav a, .text-nav a').on('mouseleave', function(e) {
        var c = $(this).attr('href').substring(1);
        $('li.' + c).removeClass('hover');
    });
    
    $('.icon-nav a, .text-nav a').on('click', function(e) {
        e.preventDefault();
        
        if(!$(this).parent().hasClass('active')) {
            $('.icon-nav > li, .text-nav > li, .content1 .content-wrapper > div, .content2 .content-inner-wrapper > div').removeClass('active');
            
            var c = $(this).attr('href').substring(1);
            $('li.' + c + ', .content1 .content-wrapper > div.' + c + ', .content2 .content-inner-wrapper > div.' + c).addClass('active');
        }
    });
});
</script>

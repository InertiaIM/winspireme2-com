{% extends 'InertiaWinspireBundle:Page:display.html.twig' %}
{% block title %}"No Risk" Auction Items | Fundraiser Auction Items | Winspire{% endblock %}
{% block description %}Auction Items Made Easy! Winspire gives you powerful tools to find the best priceless Experiences to sell at your next fundraising auction with no risk.{% endblock %}
{% block style %}
<style>
.inner-wrapper {
    background: #fff;
    border:1px solid #dddddd;
    padding:260px 70px 58px 70px;
    position:relative;
}

.banner {
    width:914px;
    height:299px;
    display:block;
    position:absolute;
    background:url(/uploads/pages/how-winspire-works/banner.png) no-repeat;
    background-size: 914px 299px;
    top:-45px;
    left:0px;
}

.clouds {
    width:264px;
    height:165px;
    display:block;
    position:absolute;
    background:url(/uploads/pages/how-winspire-works/clouds.png) no-repeat;
    background-size: 264px 165px;
    top:-12px;
    right:7px;
}

ul.blocks {
    display:block;
    position:absolute;
    top:-88px;
    left:67px;
}

ul.blocks li {
    float:left;
    display:block;
    list-style:none;
    width:158px;
    height:198px;
    margin-right:18px;
    padding:16px 20px;

    background: rgb(106,146,196);
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(top,  rgba(106,146,196,1) 0%, rgba(0,93,159,1) 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(106,146,196,1)), color-stop(100%,rgba(0,93,159,1)));
    background: -webkit-linear-gradient(top,  rgba(106,146,196,1) 0%,rgba(0,93,159,1) 100%);
    background: -o-linear-gradient(top,  rgba(106,146,196,1) 0%,rgba(0,93,159,1) 100%);
    background: -ms-linear-gradient(top,  rgba(106,146,196,1) 0%,rgba(0,93,159,1) 100%);
    background: linear-gradient(to bottom,  rgba(106,146,196,1) 0%,rgba(0,93,159,1) 100%);

    border:2px solid #fff;

    -webkit-box-shadow:  0px 1px 2px 0px rgba(0, 0, 0, 0.5);
    box-shadow:  0px 1px 2px 0px rgba(0, 0, 0, 0.5);
}

ul.blocks li .icon {
    display:block;
    text-align:center;
    font-size:54px;
    line-height:63px;
    font-weight:normal;
    color:#fff;
    text-shadow: 0px -1px 0px rgba(0, 0, 0, 0.5);
    margin-bottom:6px;
}

ul.blocks li .icon.icon-speech-check {
    font-size:49px;
    line-height:63px;
}

#standard-page ul.blocks li h4 {
    color:#fff3be;
    font-size:20px;
    line-height:24px;
    text-shadow: 0px -1px 0px rgba(0, 0, 0, 0.5);
    margin-bottom:4px;
}

#standard-page ul.blocks li p {
    color:#fff;
    font-size:12px;
    line-height:20px;
}

#standard-page .wrapper {
    margin-top:140px;
    padding-top:203px;
    padding-bottom:44px;
}

#standard-page p {
    color:#565656;
    font-size:17px;
}

#standard-page .wrapper h2 {
    font-size:36px;
    color:#28466c;
    text-align:center;
    margin-bottom:11px;
}

#standard-page .wrapper h4 {
    color:#565656;
    font-size:20px;
    margin-bottom:13px;
}

#standard-page .wrapper h4 em {
    font-size:24px;
    font-style:normal;
}

#standard-page .wrapper h4 .icon {
    color:#777777;
    font-size:28px;
    vertical-align:-2px;
    text-shadow: 0px -1px 0px rgba(0, 0, 0, 0.75);
}

#standard-page .wrapper h4 .icon.icon-megaphone {
    margin-right:11px;
}

#standard-page .wrapper h4 .icon.icon-speech-check {
    font-size:25px;
    margin-right:6px;
    vertical-align:-4px;
}

#standard-page .wrapper h4 .icon.icon-trophy {
    margin-right:8px;
}

#standard-page .wrapper p {
    font-size:14px;
    line-height:24px;
}

#standard-page .wrapper p strong {
    font-weight:normal;
    color:#0578c8;
}

#standard-page .wrapper a {
    font-size:14px;
    font-weight:bold;
    line-height:25px;
    color:#cb6700;
    text-decoration:none;
}

#standard-page .wrapper a:hover {
    color:#0578c8;
}

#standard-page .wrapper a .icon {
    font-size:20px;
    margin-left:-2px;
    vertical-align:middle;
}

.wrapper .col2 {
    padding:24px 0;
}

.wrapper .col2 img {
    float:left;
}

.wrapper .col2 .text {
    padding-left:20px;
    width:425px;
    float:left;
}

#standard-page .wrapper hr {
    display:block;
    background:none;
    border:none;
    border-bottom:1px dashed #d9d9d9;
    margin:0 100px;
}

#standard-page .page-footer {
    margin:54px 101px 0 101px;
    width:572px;
    height:49px;
    background:url(/uploads/pages/about-us/logo-small.png) no-repeat;
    background-size:572px 49px;
}
</style>
{% endblock %}

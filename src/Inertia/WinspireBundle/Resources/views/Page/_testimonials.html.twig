<script>
$(function() {
    $('#standard-page').on('click', 'a[href="#nonprofits"], a[href="#winning-bidders"]', function(e) {
        e.preventDefault();
        
        // If using the links in the footer, we'll
        // scroll back to the top of the page.
        if($(this).parent().is('h5')) {
            $(window).scrollTop(0);
        }
        
        var c = $(this).attr('href').substring(1);
        if (!$('.nav-wrapper .nav a[href="#' + c + '"]').hasClass('active')) {
            $('.wrap').removeClass('active');
            $('#' + c).addClass('active');
            
            $('.nav-wrapper .nav a').removeClass('active');
            $('.nav-wrapper .nav a[href="#' + c + '"]').addClass('active');
            
            if (c == 'nonprofits') {
                $('.testimonial-footer .more h5 a').attr('href', '#winning-bidders');
                $('.testimonial-footer .more h5 a .text').text('Winning Bidder Testimonials');
            }
            else {
                $('.testimonial-footer .more h5 a').attr('href', '#nonprofits');
                $('.testimonial-footer .more h5 a .text').text('Nonprofit Testimonials');
            }
        }
    });
});
</script>

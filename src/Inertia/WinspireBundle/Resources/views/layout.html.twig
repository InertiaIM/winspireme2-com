    <head>
    	
    	<title>{% block title %}Winspire{% endblock %}</title>
    	
    	<!-- Meta -->
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width">
        
        <!-- Styles -->
        <link rel="stylesheet" href="/css/normalize.min.css">
        {% stylesheets output="css/style.css"
            'css/less/main.less'
            'css/less/misc.less'
            'css/less/icom.less'
        %}
        <link rel="stylesheet" href="{{ asset_url }}" media="screen">
        {% endstylesheets %}
        <!--[if gte IE 9]><style type="text/css"> .gradient { filter: none; } </style><![endif]-->
        
        <!-- Scripts -->
        <script src="/js/vendor/jquery-1.8.2.min.js"></script>
        <script src="/js/vendor/modernizr-2.6.1.min.js"></script>
        
    </head>
    <body>
        <!--[if lt IE 7]><p class="chromeframe">You are using an outdated browser. <a href="http://browsehappy.com/">Upgrade your browser today</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to better experience this site.</p><![endif]-->
        
        
        
                
        <div class="container">
        	<header>
        		
        		<div id="nav-core">
        			<div class="c-wrap">
        			
	        			<a href="#" id="core-home">
	        				<img src="/img/ws-logo.png" />
	        			</a>
	        			<div id="core-suitcase">
        					<span aria-hidden="true" class="ico-briefcase icom-icon"></span>
	        				<div class="btn-label"><em>my</em><strong>Suitcase</strong></div>
	        				<img class="btn-drop" src="/img/ws-core-suitcase-dropbtn.png" />
	        			</div>
	        			<nav id="core-nav">
	        				<ul class="inline-nav">
	        					<li><a href="#">About Us</a></li>
	        					<li><a href="#">Blog</a></li>
	        					<li><a href="#">FAQs</a></li>
	        					<li><a href="#">Contact Us</a></li>
	        					<li><a href="#">My Account</a></li>
	        				</ul>
	        			</nav>
	        			
        			</div><!-- end c-wrap -->
        		</div><!-- end nav-core -->
        		
        		<div id="nav-site">
        			<div class="c-wrap">
        			
	        			<nav id="site-nav">
	        				<ul class="inline-nav">
	        					<li><a href="#">Experiences</a></li>
	        					<li><a href="#">How it Works</a></li>
	        					<li><a href="#">Why Winspire</a></li>
	        					<li><a href="#">Winning Bidders</a></li>
	        				</ul>
	        			</nav>
	        			
	        			<div id="site-search">
	        				<input type="text" class="search-text" value="Search" />
	        				<input type="button" class="search-submit" value="" />
	        			</div>

        			</div><!-- end c-wrap -->
        		</div><!-- end nav-site -->

        	
        	</header>
        	
        	
        	<div class="content">
        		
        		<div class="c-wrap">

	            {% block content %}
	            {% endblock %}
	            
            	
            	</div>
            	
            </div>
            
            
            <footer>
            	
            	<div class="c-wrap">
	            	
	            	[social]
	            	
	            	[nav]
	            	
	            	[tos/pp/cst]
	            	
	               
	            	&copy; {{ 'now' | date('Y') }} Winspire All Rights Reserved
	            	
            	</div>
            	
            </footer>
        </div> <!-- /container -->
        
        
        
        
        
        
        
        
        <script src="/js/main.js"></script>

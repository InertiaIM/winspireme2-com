							<li><a href="{{ path('packageList') }}">Experiences</a></li>
							<li><a href="{{ path('pageDisplay', {'slug': 'how-winspire-works'}) }}">How Winspire Works</a>
								<ul>
									<li><a href="{{ path('pageDisplay', {'slug': 'faqs'}) }}">FAQs</a></li>
								
								</ul>
							</li>
							<li><a href="{{ path('pageDisplay', {'slug': 'why-winspire'}) }}">Why Winspire</a>
								<ul>
									<li><a href="{{ path('pageDisplay', {'slug': 'about-us'}) }}">About Us</a></li>
									
									<li><a href="{{ path('pageDisplay', {'slug': 'testimonials'}) }}">Testimonials</a></li>
								
								</ul>
							</li>
							<li><a href="{{ path('pageDisplay', {'slug': 'winning-bidders'}) }}">Winning Bidders</a></li>
							<li><a href="{{ path('contactUs') }}">Contact Us</a>
								<ul>
									<li><a href="http://blog.winspireme.com">Blog</a></li>
								</ul>
							</li>
						</ul>
					</nav>
				</div>
                
                <div id="nav-legal">
                    <nav id="legal-nav">
                        <ul class="inline-nav">
                            <li><a href="{{ path('pageDisplay', {'slug': 'terms'}) }}">Terms of Use</a></li>
                            <li><a href="{{ path('pageDisplay', {'slug': 'privacy'}) }}">Privacy Policy</a></li>
{% if app.user and is_granted('ROLE_USER') %}
                            <li><a href="{{ path('pageDisplay', {'slug': 'loa'}) }}">Letter of Agreement</a></li>
{% endif %}
                            <li>CST# 2094258-40</li>
                            <li><span id="legal-copyright"> <a href="{{ path('pageDisplay', {'slug': 'copyright'}) }}">&copy; {{ 'now' | date('Y') }} Winspire All Rights Reserved</a> </span></li>
                        </ul>
                    </nav>

                    <div id="country-select">
                        <dl class="dropdown">
                            <dt>
{% if app.request.locale == 'us' %}
                                <a href="#"><span class="flag us"></span><em>United States</em></a>
{% endif %}
{% if app.request.locale == 'ca' %}
                                <a href="#"><span class="flag ca"></span><em>Canada</em></a>
{% endif %}
                            </dt>
                            <dd>
                                <ul>
                                    <li>
                                        <a href="{{ urls['us'] }}?_locale=us"><span class="flag us"></span><em>United States</em></a>
                                    </li>
                                    <li>
                                        <a href="{{ urls['ca'] }}?_locale=ca"><span class="flag ca"></span><em>Canada</em></a>
                                    </li>
                                </ul>
                            </dd>
                        </dl>
                    </div>
                </div>
            </div>
        </footer>
    </div>

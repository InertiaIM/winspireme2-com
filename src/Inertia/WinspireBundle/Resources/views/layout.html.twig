                            <li><a href="{{ path('pageDisplay', {'slug': 'faqs'}) }}">FAQs</a></li>
                            <li><a href="{{ path('contactUs') }}">Contact Us</a></li>
{% if app.user and is_granted('ROLE_ADMIN') %}
                            <li><a href="{{ path('suitcaseAdmin') }}">Admin</a></li>
{% endif %}

                        </ul>
                    </nav>
				</div>
				<!-- end c-wrap -->
			</div>
			<!-- end nav-core -->
{% render controller('InertiaWinspireBundle:Default:siteNav') %}
		</header>

		<div class="content">
			{% block content %} {% endblock %}
		</div>

        <footer>
            <div class="c-wrap">

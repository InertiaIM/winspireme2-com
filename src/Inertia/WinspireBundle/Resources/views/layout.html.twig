<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js">
<!--<![endif]-->
<head>
<title>{% block title %}Winspire{% endblock %}</title>

<!-- Meta -->
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<meta name="description" content="">
<meta name="viewport" content="width=device-width">

<!-- Styles -->
<link rel="stylesheet" href="/css/normalize.min.css">
{% stylesheets output="css/style.css"
'css/less/main.less'
'css/less/icom.less'
%}
<link rel="stylesheet" href="{{ asset_url }}" media="screen">
{% endstylesheets %}
<!--[if gte IE 9]><style type="text/css"> .gradient { filter: none; } </style><![endif]-->

<!-- Scripts -->
<script src="/js/vendor/jquery-1.8.2.min.js"></script>
<script src="/js/vendor/modernizr-2.6.1.min.js"></script>
<script src="/js/vendor/jquery.tools.min.js"></script>
<script src="/js/vendor/jquery.carousel.js"></script>
<script src="/js/vendor/jquery.cycle2.min.js"></script>
<script src="/js/vendor/jquery.cycle2.carousel.min.js"></script>
<script src="/js/vendor/jquery.cycle2.swipe.min.js"></script>
<script src="/js/vendor/waypoints.min.js"></script>

<script src="/js/vendor/jmpress.min.js"></script>
<script src="/js/jquery.homeslideshow.js"></script>
</head>
<body>
	<!--[if lt IE 7]><p class="chromeframe">You are using an outdated browser. <a href="http://browsehappy.com/">Upgrade your browser today</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to better experience this site.</p><![endif]-->

	<div class="container">
		<header>

			<div id="nav-core">
				<div class="c-wrap">

					<a href="{{ path('home') }}" id="core-home"> <img src="/img/ws-logo.png" alt="Winspire"/>
					</a>
					<div id="core-suitcase">
					
						<div id="suitcase-toggle">
							<div class="btn-label">
							    <span aria-hidden="true" class="ico-suitcase"></span>
								<em>my</em><strong>Suitcase</strong>
							</div>
							<div class="btn-drop">
								<span aria-hidden="true" class="ico-arrow-down"></span>
							</div>
						</div>
						
						<!-- Suitcase Drop -->
						<div id="suitcase-panel">
							<div class="panel-tab">
								<div class="the-tab">
									<div class="tab-btn">
										<span aria-hidden="true" class="ico-arrow-down"></span>
									</div>
								</div>
								<div class="the-panel">
									
									<div class="panel-email panel-field">
										<input type="text" class="pf-text" value="Email Address" />
									</div>
								
									<div class="panel-password panel-field">
										<input type="password" class="pf-text" value="Password" />
										<input type="button" class="pf-button" value="Forgot?" />
									</div>
									
									<div class="panel-buttons">
										<input type="button" class="pa-button pa-su" value="Sign Up" />
										<input type="button" class="pa-button pa-li" value="Log In" />
										<div class="reset"></div>
									</div>
									
								</div>
							</div>
							
							
						</div>
						<!-- Suitcase Drop -->
						
					</div>
					<nav id="core-nav">
						<ul class="inline-nav">
							<li><a href="#">About Us</a></li>
							<li><a href="#">Blog</a></li>
							<li><a href="#">FAQs</a></li>
							<li><a href="#">Contact Us</a></li>
						</ul>
					</nav>

				</div>
				<!-- end c-wrap -->
			</div>
			<!-- end nav-core -->
{% render "InertiaWinspireBundle:Default:siteNav" with {'max': 3} %}
		</header>

		<div class="content">
			{% block content %} {% endblock %}
		</div>


		<footer>
			<div class="c-wrap">
				<div id="nav-social">
					<img src="/img/ws-ftr-socialbg.png" class="social-bg" alt=""/>
					<nav id="social-nav">
						<ul class="inline-nav">
							<li><a href="#"><span aria-hidden="true"
									class="ico-facebook"></span> </a></li>
							<li><a href="#"><span aria-hidden="true"
									class="ico-twitter"></span> </a></li>
							<li><a href="#"><span aria-hidden="true"
									class="ico-youtube"></span> </a></li>
							<li><a href="#"><span aria-hidden="true"
									class="ico-envelope"></span> </a></li>
						</ul>
					</nav>
				</div>

				<div id="nav-sitemap">
					<nav id="sitemap-nav">
						<ul class="inline-nav">
							<li><a href="{{ path('packageList') }}">Experiences</a></li>
							<li><a href="#">How Winspire Works</a>
								<ul>
									<li><a href="#">FAQs</a></li>
								
								</ul>
							</li>
							<li><a href="#">Why Winspire</a>
								<ul>
									<li><a href="#">About Us</a></li>
									
									<li><a href="#">Testimonials</a></li>
								
								</ul>
							</li>
							<li><a href="#">Winning Bidders</a></li>
							<li><a href="#">Contact Us</a>
								<ul>
									<li><a href="#">Resources</a></li>
									
									<li><a href="#">Blog</a></li>
								
								</ul>
							</li>
						</ul>
					</nav>
				</div>

				<div id="nav-legal">
					<nav id="legal-nav">
						<ul class="inline-nav">
							<li><a href="#">Terms of Use</a></li>
							<li><a href="#">Privacy Policy</a></li>
							<li><a>CST# 2094258-40</a></li>
						</ul>
					</nav>

					<span id="legal-copyright"> &copy; {{ 'now' | date('Y') }} Winspire All Rights Reserved </span>
				</div>
			</div>

		</footer>
	</div>
	<!-- /container -->

	
	
	<script src="/js/main.js"></script>
